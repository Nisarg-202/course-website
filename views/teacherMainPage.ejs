
<%- include("partials/header"); -%>

<h1>Add Course</h1>
   <form action="/publish" method="post">
     <div class="form-group">
       <label>Your Name</label>
       <input class="form-control" type="text" name="teacherName" autocomplete="off" required>
       <label>Title</label>
       <input class="form-control" type="text" name="postTitle" autocomplete="off" required>
       <label>Description</label>
       <textarea class="form-control" name="postBody" rows="5" cols="30" required></textarea>
     </div>
     <button class="btn btn-primary" type="submit" name="button">Publish</button>
   </form>

<%- include("partials/footer"); -%>
